#include <stdio.h>

int main() 
    {
    int n,i,j;
    printf("Enter the size of the array: ");
    scanf("%d",&n);

    int a[n];
    for (i=0;i<n;i++) 
        {
        printf("element %d: ",i);
        scanf("%d", &a[i]);
        }
//prime checking
    for(i=0;i<n;i++)
        {
        int count=0;
        for(j=1;j<a[i];j++)
            {
            
            if(a[i]%j==0)
                {
                count++;
                }
            }
        if (count==1)
            {
            a[i]=0;
            int m;
            m++
            }
        }

     

    printf("The rearranged array: ");
    for(i=0;i<m;i++) 
        {
        printf("%d",a[i]);
        }

    return 0;
    }
